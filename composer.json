{
    "name": "zenadmin/zenadmin",
    "description": "ZenAdmin WordPress Plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "lucatume/wp-browser": "^3.0",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=WordPress-Core includes/ zenadmin.php"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
